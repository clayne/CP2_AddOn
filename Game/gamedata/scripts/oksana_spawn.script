	end
end

function akim_panic()
	if shax_start() then
		news_manager.send_tip("%c[gray2]".."АКИМ:".."\\n".."%c[red_sms]$actor, ноги в руки и мелкими перебежками через огороды ко мне! Весь Кордон на измене! Кто-то у меня на КПП нейтралов порезал и оставил записку: Привет от Шахматиста!", 5, nil, 30000)
	end
end
function shax_start()
	return has_alife_info("tel_dcity_kanaliy") and has_alife_info("volna_day4_done")
end
