function change_community()
	local sanka_kodla = {
		story_ids.zat_kidalo2,
		story_ids.kidalo_friend1,
		story_ids.kidalo_friend2,
		story_ids.kidalo_friend3
	}
	local npc
	for i = 1,#sanka_kodla do
		npc = level_object_by_sid( sanka_kodla[i] )
		if npc and npc:alive () then
			npc:set_character_community("bandit", 0, 0)
		end
	end
end

