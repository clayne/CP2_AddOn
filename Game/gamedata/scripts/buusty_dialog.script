--проверка ноутбука в рюкзаке
function aaz_note_have()
return inventory.search("notebook_aaz",1) ~= false 
end

--отдадим ноут Аазу
function otday_note_aaz(first_speaker, second_speaker)
inventory.out_items("notebook_aaz",1)
second_speaker:set_relation(game_object.friend, first_speaker)
end

