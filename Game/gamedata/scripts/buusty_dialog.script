local loot_money = nil
function money_loot_have(fs, ss)
	if loot_money == nil then
		loot_money = 0
		Actor:iterate_inventory( function (actor, item)
			if item:section() == "money_loot" then
				loot_money = loot_money + math.random(200,800)
			end
		end, 
		Actor )
		xr_s.subscribe( "talk_close", function() loot_money = nil end, {once = true} )
	end

	return loot_money > 0
end

function exchange_money(fs, ss)
	if loot_money == nil then
		money_loot_have(fs, ss)
	end
	inventory.out_items_all("money_loot")
	aem_manager.manage_money(loot_money, "in")
	archievements.acv_count_event("acv_mar", 50000, "Мародёр", loot_money)
	loot_money = nil
end
function money_loot_show(fs, ss)
	if loot_money == nil then
		money_loot_have(fs, ss)
	end
	local rnd_phrase = {"Всего получается","Насобирал ты","Здесь","У тебя","В общем получается","Всего здесь","Ага,"}
	local task_texture, task_rect = get_texture_info("ui_iconsTotal_abstract5")
	local text = table.random(rnd_phrase).." "..loot_money..". Меняешь?"
	Actor:give_talk_message(text, task_texture, task_rect, "simple_answer_item")
	return true
end
	
